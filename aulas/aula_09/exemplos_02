frase = ' Curso em Vídeo Python'

#conta letra O
print(frase.count('o'))
#Escreve em maiuscula
print(frase.upper())
#tamanho da frase
print(len(frase))
#exclui espaço em branco
print(frase.strip())
#troca uma palavra por outra
print(frase.replace('Python', 'Romulo'))
#mostra o caracter 3
print(frase[3])
#existe 'curso' na frase?
print('Curso' in frase)
#procure 'curso'
print(frase.find('Curso'))
# divide as palavras em lista
print(frase.split())
